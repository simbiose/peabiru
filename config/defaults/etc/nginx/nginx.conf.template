daemon            off;
pid               ${SHARED}/pids/nginx.pid;
user              www-data www-data;
worker_processes  1;

events { worker_connections  1024; }

http {
  include            mime.types;
  default_type       application/octet-stream;
  sendfile           on;
  keepalive_timeout  65;
  gzip               on;

  include /etc/nginx/conf.d/*.conf;
}
